{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "git_merge_conflict",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_body"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_body"
                },
                {
                  "type": "SYMBOL",
                  "name": "conflict"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_body"
                },
                {
                  "type": "SYMBOL",
                  "name": "comment"
                }
              ]
            }
          ]
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_body": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_eol"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "conflict"
              },
              {
                "type": "SYMBOL",
                "name": "_eol"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "_eol"
              }
            ]
          }
        ]
      }
    },
    "conflict": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "ours",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "_body"
                },
                "named": true,
                "value": "hunk"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "common_sep"
                },
                {
                  "type": "SYMBOL",
                  "name": "_eol"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "common",
                      "content": {
                        "type": "ALIAS",
                        "content": {
                          "type": "SYMBOL",
                          "name": "_body"
                        },
                        "named": true,
                        "value": "hunk"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "sep"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "theirs",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "_body"
                },
                "named": true,
                "value": "hunk"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end"
        }
      ]
    },
    "start": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[<]{7,}[ \\t]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "label",
                "content": {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\\r\\n]+"
                  },
                  "named": true,
                  "value": "label"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "common_sep": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[|]{7,}[ \\t]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "label",
                "content": {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\\r\\n]+"
                  },
                  "named": true,
                  "value": "label"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "sep": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[=]{7,}[ \\t]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "label",
                "content": {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\\r\\n]+"
                  },
                  "named": true,
                  "value": "label"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "end": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[>]{7,}[ \\t]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "label",
                "content": {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\\r\\n]+"
                  },
                  "named": true,
                  "value": "label"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_eol": {
      "type": "PATTERN",
      "value": "[\\r\\n]|\\r\\n"
    },
    "comment": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_char"
      }
    },
    "_char": {
      "type": "PATTERN",
      "value": "[^\\r\\n]"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
